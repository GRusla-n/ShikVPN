# ShikVPN Client Configuration

# Server IP or hostname
server = "your.server.ip"

# Client private key (generate with vpn-keygen)
private_key = "YOUR_CLIENT_PRIVATE_KEY_BASE64"

# Interface MTU
mtu = 1420

# Keepalive interval in seconds (helps with NAT traversal)
persistent_keepalive = 25

# TUN interface name (optional, default: wg0)
# interface_name = "wg0"
